  public static void main(String[] args){
    System.out.printf("%-10.10s  %-10.10s%n", "James", "August 5th");
    System.out.printf("%-10.10s  %-10.10s%n", "Adam", "April 23rd");
    System.out.printf("%-10.10s  %-10.10s%n", "Nick", "June 12th");
  }
